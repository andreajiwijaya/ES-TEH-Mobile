{
  "expo": {
    "name": "Es Teh Favorit Indonesia",
    "slug": "es-teh",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-esteh.png",
    "scheme": "esteh",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.esteh.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-esteh.png",
        "backgroundColor": "#F0F4F8"
      },
      "package": "com.esteh.app",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/logo-esteh.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo-esteh.png",
          "imageWidth": 150,
          "resizeMode": "contain",
          "backgroundColor": "#F0F4F8"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}